    "eslint": "^9",
    "eslint-config-next": "^15.1.3",
    "eslint-config-prettier": "^9",
    "eslint-plugin-check-file": "^2.8.0",
    "husky": "^9.1.6",
    "jsdom": "^25.0.1",
    "postcss": "^8",
    "prettier": "^3.4.2",
